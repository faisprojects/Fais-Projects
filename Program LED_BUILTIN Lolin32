//Menyalakan LED Bawaan ESP32

void setup(){
  pinMode(LED_BUILTIN, OUTPUT);
  //inisialisasi LED_BUILTIN sebagai output
}
 
void loop(){
  digitalWrite(LED_BUILTIN, HIGH);
  //perintah untuk mematikan 
  delay(1000);
  //waktu jeda 1 detik
  digitalWrite(LED_BUILTIN, LOW);
  //perintah untuk menyalakan
  delay(1000);
  //waktu jeda 1 detik
}
